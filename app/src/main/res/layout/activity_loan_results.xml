<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoanResults">
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/sbi"
        android:id="@+id/sbi"
        android:layout_marginRight="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="50dp"
        />
    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/tickmark"
        android:id="@+id/sbitick"
        android:layout_toRightOf="@+id/sbi"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="25dp"
        android:visibility="invisible"
        />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/hdfc"
        android:id="@+id/hdfc"
        android:layout_marginRight="20dp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/sbi"
        android:layout_marginLeft="50dp"
        />
    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/tickmark"
        android:id="@+id/hdfctick"
        android:layout_toRightOf="@+id/hdfc"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="85dp"
        android:layout_below="@+id/sbitick"
        android:visibility="invisible"
        />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/icici"
        android:id="@+id/icici"
        android:layout_marginRight="20dp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/hdfc"
        android:layout_marginLeft="50dp"
        />
    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/tickmark"
        android:id="@+id/icicitick"
        android:layout_toRightOf="@+id/icici"
        android:layout_marginLeft="150dp"
        android:layout_below="@+id/hdfctick"
        android:layout_marginTop="75dp"
        android:visibility="invisible"
        />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/axis"
        android:id="@+id/axis"
        android:layout_marginRight="20dp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/icici"
        android:layout_marginLeft="50dp"
        />
    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/tickmark"
        android:id="@+id/axistick"
        android:layout_toRightOf="@+id/axis"
        android:layout_marginLeft="150dp"
        android:layout_below="@+id/icicitick"
        android:layout_marginTop="75dp"

        android:visibility="invisible"/>
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/kotak"
        android:id="@+id/kotak"
        android:layout_marginRight="20dp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/axis"
        android:layout_marginLeft="50dp"
        />
    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/tickmark"
        android:id="@+id/kotaktick"
        android:layout_toRightOf="@+id/axis"
        android:layout_marginLeft="150dp"
        android:layout_below="@+id/axistick"
        android:layout_marginTop="65dp"
        android:visibility="invisible"
        />



</RelativeLayout>